{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Condition": {
        "StringNotLike": {
          "aws:PrincipalARN": [
            "arn:aws:iam::*:role/OrganizationAccountAccessRole",
            "arn:aws:iam::*:role/AWSControlTowerExecution",
            "arn:aws:iam::*:role/AWSAccelerator-*"            
          ]
        },
        "StringNotEquals": {
          "aws:RequestedRegion": [
            "us-east-2",
            "us-west-2"
          ]
        }
      },
      "Resource": "*",
      "Effect": "Deny",
      "NotAction": [
        "iam:*",
        "cloudtrail:*",
        "organizations:*",
        "route53:*",
        "budgets:*",
        "waf:*",
        "wafv2:*",
        "cloudfront:*",
        "globalaccelerator:*",
        "importexport:*",
        "support:*"
      ],
      "Sid": "PreventResourcesOutsideApprovedRegions"
    },
    {
      "Action": [
        "acm:*",
        "acm-pca:*",
        "apigateway:*",
        "application-autoscaling:*",
        "appstream:*",
        "athena:*",
